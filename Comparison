#include <stdio.h>
   
   int main() {
       
       int a = 4, b = 8;
       
           printf("%d\n", a > b);
//0
           printf("%d\n", a < b);
//1
           printf("%d\n", a >= b);
//0
           printf("%d\n", a <= b);
//1
           printf("%d\n", a == b);
//0
           printf("%d\n", a != b);
//1

   return 0;
   }
