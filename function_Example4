#include <stdio.h>

//function to detect prime number
int prime(int n){
    for(int i = 2; i < n; ++i){
        if(n % i == 0)
          return 0;
          //return 0 if non-prime
    }
    return 1;
    //return 1 if prime
  }
 
int main(){
int n = 4, isPrime;
isPrime = prime(n);
    if(isPrime == 1){
printf("%d es primo", n);
}
    else{
printf("%d no es primo", n);
}
return 0;
}
