//Access array elements
#include <stdio.h>
int main(){
    int data[] = {12, 19, 13, 12, 17};
    printf("1st element: %d\n", data[0]);
    //1st element: 12
    printf("2nd element: %d\n", data[1]);
    //2nd element: 19
    printf("5th element: %d\n", data[4]);
    //5th element: 17
    return 0;
}
