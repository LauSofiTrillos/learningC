// strcmp() compara dos strings
// si las Strings son iguales retorna 0
// si las Strings son diferentes retorna un valor no cero 
#include <stdio.h>
#include <string.h>
int main(){

  char str1[] = "a,b,c,d", str2[] = "cdEA";
  int result;
  
  result = strcmp(str1, str2);
  printf("Result = %d\n", result);
  
  if(result == 0){
   printf("strings are equal");
  }
  else {
   printf("not equal");
  }
  return 0;
  }
