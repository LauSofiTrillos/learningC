#include <stdio.h>
 
   int main(){
   
       double num;
       
       for(int i = 1; i <= 10; ++i){
       
       printf("Number: ");
       scanf("%lf", &num);
       
       if(number < 0.0){
       break;
       }
      
       printf("Number = %.2lf", num);
       }
       
    return 0;
   }
