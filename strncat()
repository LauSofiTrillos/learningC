// ejemplo implementacion de strncat()
// concatena 2 Strings 
// strncat(destino, fuente, n)
// n  = cantidad de caracteres a concatenar desde la fuente, a la String destino

#include <stdio.h>
#include <string.h>

int main(){

char dest[20]="Hello";
char src[20]="World";

 strncat(dest,src,3);
  printf("%s\n",dest); //HelloWor
 strncat(dest,src,20);
  printf("%s\n",dest); //HelloWorld

return 0;
}
