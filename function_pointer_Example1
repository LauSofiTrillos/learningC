Swap two numbers using Pointers 
#include <stdio.h>
  void swapNumbers(int *n1, int *n2){
    int temp;
    //swap values 
    temp = *n1;
    *n1 = *n2;
    *n2 = temp;  
  }
  int main(){
  
   int num1 = 1, num2 = 100;
   swapNumbers(&num1, &num2);
   printf("num1: %d", num1); //100
   printf("num2: %d", num2); //1
  
  return 0;
  }
