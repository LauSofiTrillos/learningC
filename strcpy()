
// ejemplo strcpy(), syntax: strcpy(destino, fuente)
// El destino y la fuente son tipo Char

#include <stdio.h>
#include <string.h>

int main(){

  char str1[10] = "awesome";
  char str2[10];
  char str3[10];
  
  strcpy(str2, str1);
  strcpy(str3, "well");
  
  puts(str2); // awsome 
  puts(str3); // well
  
  return 0;  
}
// la string de destino debe tener tama√±o suficiente para almacenar la String de fuente
